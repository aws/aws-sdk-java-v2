        "headers": {
          "Content-Type": "application/x-amz-json-1.0"
        },
        "body": "{\n    \"__type\": \"aws.protocoltests.json10#NoCustomCodeError\",\n    \"Message\": \"Hi\"\n}"
      }
    },
    "when": {
      "action": "unmarshall",
