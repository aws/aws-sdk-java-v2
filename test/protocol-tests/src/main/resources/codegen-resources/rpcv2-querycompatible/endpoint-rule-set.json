{
    "version": "1.3",
    "parameters": {
        "Region": {
            "builtIn": "AWS::Region",
            "required": true,
            "documentation": "The AWS region used to dispatch the request.",
            "type": "String"
        }
    },
    "rules": [
        {
            "conditions": [],
            "endpoint": {
                "url": "http://localhost/",
                "properties": {
                    "authSchemes": [
                        {
                            "name": "sigv4",
                            "signingRegion": "{Region}",
                            "signingName": "jsonrpc"
                        }
                    ]
                },
                "headers": {}
            },
            "type": "endpoint"
        }
    ]
}
