{
  "version": "1.1",
  "service": "headers",
  "testCases": [
    {
      "documentation": "header set to region",
      "params": {
        "Region": "us-east-1"
      },
      "expect": {
        "url": "https://us-east-1.amazonaws.com",
        "headers": {
          "x-amz-region": [
            "us-east-1"
          ],
          "x-amz-multi": [
            "*",
            "us-east-1"
          ]
        }
      }
    }
  ]
}
