{
    "calculateCrc32FromCompressedData": true,
    "sdkModeledExceptionBaseClassName": "DynamoDbException",

    // exclude these shapes that are already present in the dynamodb client
    "shapeModifiers": {
        "AttributeValue" : {
            "excludeShape": true
        },
        "KeySchemaElement" : {
            "excludeShape": true
        },
        "KeyType" : {
            "excludeShape": true
        },
        "StreamViewType" : {
            "excludeShape": true
        },
        "InternalServerError" : {
            "excludeShape": true
        },
        "LimitExceededException" : {
            "excludeShape": true
        },
        "ResourceNotFoundException" : {
            "excludeShape": true
        }
    },
    "shareModelConfig" : {
        "shareModelWith" : "dynamodb",
        "packageName" : "streams"
    },
    "verifiedSimpleMethods" : [
        "listStreams"
    ]
}
